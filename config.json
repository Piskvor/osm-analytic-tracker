{
    "path": "html",
    "tracker": {
	"osm_api_url": "https://api.openstreetmap.org",
	"pre_labels": [
	    {"area_file": "/osm-regions/praha.poly", "area_check_type": "cset-bbox", "label": "inside-area"},
	    {"area_file": "/osm-regions/praha.poly", "area_check_type": "cset-center", "label": "center-inside-area"}
	],
	"prefilter_labels": [["inside-area", "center-inside-area"]],
	"post_labels": [
	    {"regex": [{".changes.tag.osak:identifier": ""}], "label": "address-node-change"},
        {"regex": [{".meta.comment": "([fF]ix)|([oO]prava)|([rR]evert)"}], "label": "fix"},
        {"regex": [{".changes.tag.created_by": "maps.me"}], "label": "mm"},
        {"regex": [{".meta.comment": "mm"}], "label": "mm"}
	],
	"history": "5 minutes ago",
	"horizon_type": "sliding",
	"horizon_hours": 48,
	"refresh_open_minutes": 5,
	"refresh_meta_minutes": 15,
	"cset_processing_time_max_s": 800,
	"template_path": "templates"
    },
    "backends": [
        {
            "type": "BackendHtml",
            "init_only": true,
            "map_center": {"area_file_conversion_type": "area_center",
                           "area_file": "/osm-regions/praha.poly"},
            "map_scale": "10",
            "path": "",
            "filename" : "index.html",
            "template": "index.html"
        },
        {
            "type": "BackendHtml",
            "show_details": true,
            "show_comments": true,
            "path": "dynamic",
            "page_title": "Recent Changesets",
            "filename" : "today.html",
            "template": "changeset.html"
        },
        {
            "type": "BackendHtmlSummary",
            "path": "dynamic",
            "filename" : "today-summ.html",
            "template": "summary.html"
        },
        {
            "type": "BackendGeoJson",
            "path": "dynamic",
            "exptype": "cset-bbox",
            "filename" : "today.json",
            "click_url": "diffmap.html?cid={cid}"
        },

        {
            "type": "BackendHtml",
            "init_only": true,
            "map_center": {"area_file_conversion_type": "area_center",
                "area_file": "/osm-regions/praha.poly"},
                "map_scale": "10",
            "path": "",
            "labels": ["mm"],
            "page_title": "Recent Changesets from maps.me",
            "filename" : "mm.html",
            "template": "index.html"
        },
        {
            "type": "BackendHtml",
            "show_details": true,
            "show_comments": true,
            "path": "dynamic",
            "labels": ["mm"],
            "page_title": "Recent Maps.Me edits",
            "filename" : "mm.html",
            "template": "changeset.html"
        },
        {
            "type": "BackendHtmlSummary",
            "path": "dynamic",
            "labels": ["mm"],
            "filename" : "mm-summ.html",
            "template": "summary.html"
        },
        {
            "type": "BackendGeoJson",
            "path": "dynamic",
            "labels": ["mm"],
            "exptype": "cset-bbox",
            "filename" : "mm.json",
            "click_url": "diffmap.html?cid={cid}"
        },

        {
            "type": "BackendHtml",
            "init_only": true,
            "map_center": {"area_file_conversion_type": "area_center",
                "area_file": "/osm-regions/praha.poly"},
            "map_scale": "10",
            "path": "",
            "labels": ["fix"],
            "page_title": "Recent fixes/reverts",
            "filename" : "fix.html",
            "template": "index.html"
        },
        {
            "type": "BackendHtml",
            "show_details": true,
            "show_comments": true,
            "path": "dynamic",
            "labels": ["fix"],
            "page_title": "Recent Fixes",
            "filename" : "today_fix.html",
            "template": "changeset.html"
        },
        {
            "type": "BackendHtmlSummary",
            "path": "dynamic",
            "labels": ["fix"],
            "filename" : "fix-summ.html",
            "template": "summary.html"
        },
        {
            "type": "BackendGeoJson",
            "path": "dynamic",
            "labels": ["fix"],
            "exptype": "cset-bbox",
            "filename" : "fix.json",
            "click_url": "diffmap.html?cid={cid}"
        },

        {
            "type": "BackendHtml",
            "path": "dynamic",
            "filename" : "notes.html",
            "template": "notes.html"
        },
        {
            "type": "BackendHtml",
            "path": "dynamic",
            "labels": ["address-node-change"],
            "page_title": "Recent Changesets Which Modifies Address Nodes",
            "filename" : "dk_addresses.html",
            "template": "changeset.html"
        },

        {
            "type": "BackendGeoJson",
            "path": "dynamic",
            "exptype": "cset-files",
            "geojsondiff-filename": "cset-{id}.json",
            "bounds-filename": "cset-{id}.bounds",
            "click_url": "diffmap.html?cid={cid}"
        }
    ]
}
